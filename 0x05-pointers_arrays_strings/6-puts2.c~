#include "main.h"
/**
 * puts2 - function prints every other character of a string, starting
 * with the first character, followed by a newline.
 * @str: function parameter
 *
 * Return: nothing.
 */
void puts2(char *str)
{
	int i;

	int count = 0;

	for (i = 0; s[i] != '\0'; i++)
		count++;

	for (i = 0; i < count; i += 2)
	{
		_putchar(str[i]);
	}
	_putchar('\n');
}
